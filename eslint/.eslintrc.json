{
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "env": {
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": "off",
        "object-curly-spacing": "off",
        "comma-dangle": "off",
        "max-len": "warn",
        "no-console": "warn"
    }
}
